{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Application/**.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Application",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/Common/**.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Common",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/Domain/**.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Domain",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/Infrastructure/**.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Infrastructure",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/Persistence/**.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Persistence",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "src/WebUI/**.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/WebUI",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/**/index.md",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "faq/**.md",          
          "faq/toc.yml"
        ]
      },
      {
        "files": [
          "help/**.md",
          "help/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "PropertyCore Documentation",
      "_enableSearch": "true"
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}